    "autoprefixer": "^9.5.0",
    "babel-loader": "^8.0.5",
    "chalk": "^2.4.2",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
